<%= form_for @empleado do |f| %>
    <div class="field">
      <%= f.label :emp_nom, "Nombre del Usuario" %><br />
      <%= f.text_field :emp_nom, autofocus: true %>
    </div>

    <div class="field">
      <%= f.label :emp_ape, "Apellido del Usuario" %><br />
      <%= f.text_field :emp_ape, autofocus: true %>
    </div>

    <div class="field">
      <%= f.label :emp_rut, "Rut del Usuario" %><br />
      <%= f.text_field :emp_rut, autofocus: true %>
    </div>

    <div class="field">
      <%= f.label :emp_tel, "Telefono del Usuario" %><br />
      <%= f.text_field :emp_tel, autofocus: true %>
    </div>

    <div class="field">
      <%= f.label :email, "Correo" %><br />
      <%= f.email_field :email, autofocus: true %>
    </div>

    <div class="field">
      <%= f.label :cargo_cod, "Tipo de Usuario" %><br />
      <%= collection_select(:empleado, :cargo_cod, CargoEmpleado.all, :id, :cargo_nom, prompt: true) %>
    </div>

    <p></p>
    <div class="actions">
      <%= f.submit "Guardar" %> <%= link_to "Cancelar", empleados_path %>
    </div>

<% end %>